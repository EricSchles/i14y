{
  "template": "*-i14y-documents-*",
  "settings": {
    "analysis": {
      "quotes": {
        "type": "mapping",
        "mappings": [
          "\\u0091=\u003e\\u0027",
          "\\u0092=\u003e\\u0027",
          "\\u2018=\u003e\\u0027",
          "\\u2019=\u003e\\u0027",
          "\\u201B=\u003e\\u0027"
        ]
      },
      "filter": {
        "bigram_filter": {"type": "shingle"},
        "en_synonym": {
          "type": "synonym",
          "synonyms": ["gas, petrol"]
        },
        "es_protected_filter": {
          "type": "keyword_marker",
          "keywords": ["ronaldo"]
        },
        "en_protected_filter": {
          "type": "keyword_marker",
          "keywords": ["gas"]
        },
        "de_stem_filter": {
          "type": "stemmer",
          "name": "minimal_german"
        },
        "en_stem_filter": {
          "type": "stemmer",
          "name": "minimal_english"
        },
        "fr_stem_filter": {
          "type": "stemmer",
          "name": "minimal_french"
        },
        "pt_stem_filter": {
          "type": "stemmer",
          "name": "minimal_portuguese"
        },
        "es_stem_filter": {
          "type": "stemmer",
          "name": "light_spanish"
        },
        "fi_stem_filter": {
          "type": "stemmer",
          "name": "light_finnish"
        },
        "hu_stem_filter": {
          "type": "stemmer",
          "name": "light_hungarian"
        },
        "it_stem_filter": {
          "type": "stemmer",
          "name": "light_italian"
        },
        "ru_stem_filter": {
          "type": "stemmer",
          "name": "light_russian"
        },
        "sv_stem_filter": {
          "type": "stemmer",
          "name": "light_swedish"
        },
        "ja_pos_filter": {
          "type": "kuromoji_part_of_speech",
          "stoptags": [
            "\\u52a9\\u8a5e-\\u683c\\u52a9\\u8a5e-\\u4e00\\u822c",
            "\\u52a9\\u8a5e-\\u7d42\\u52a9\\u8a5e"
          ]
        }
      },
      "es_analyzer": {
        "type": "custom",
        "filter": [
          "icu_normalizer",
          "es_protected_filter",
          "es_stem_filter",
          "es_synonym",
          "icu_folding"
        ],
        "tokenizer": "icu_tokenizer",
        "char_filter": [
          "html_strip",
          "quotes"
        ]
      },
      "en_analyzer": {
        "type": "custom",
        "filter": [
          "icu_normalizer",
          "en_protected_filter",
          "en_stem_filter",
          "en_synonym",
          "icu_folding"
        ],
        "tokenizer": "icu_tokenizer",
        "char_filter": [
          "html_strip",
          "quotes"
        ]
      },
      "de_analyzer": {
        "type": "custom",
        "filter": [
          "icu_normalizer",
          "de_stem_filter",
          "icu_folding"
        ],
        "tokenizer": "icu_tokenizer",
        "char_filter": [
          "html_strip",
          "quotes"
        ]
      },
      "fi_analyzer": {
        "type": "custom",
        "filter": [
          "icu_normalizer",
          "fi_stem_filter",
          "icu_folding"
        ],
        "tokenizer": "icu_tokenizer",
        "char_filter": [
          "html_strip",
          "quotes"
        ]
      },
      "hu_analyzer": {
        "type": "custom",
        "filter": [
          "icu_normalizer",
          "hu_stem_filter",
          "icu_folding"
        ],
        "tokenizer": "icu_tokenizer",
        "char_filter": [
          "html_strip",
          "quotes"
        ]
      },
      "it_analyzer": {
        "type": "custom",
        "filter": [
          "icu_normalizer",
          "it_stem_filter",
          "icu_folding"
        ],
        "tokenizer": "icu_tokenizer",
        "char_filter": [
          "html_strip",
          "quotes"
        ]
      },
      "pt_analyzer": {
        "type": "custom",
        "filter": [
          "icu_normalizer",
          "pt_stem_filter",
          "icu_folding"
        ],
        "tokenizer": "icu_tokenizer",
        "char_filter": [
          "html_strip",
          "quotes"
        ]
      },
      "ru_analyzer": {
        "type": "custom",
        "filter": [
          "icu_normalizer",
          "ru_stem_filter",
          "icu_folding"
        ],
        "tokenizer": "icu_tokenizer",
        "char_filter": [
          "html_strip",
          "quotes"
        ]
      },
      "sv_analyzer": {
        "type": "custom",
        "filter": [
          "icu_normalizer",
          "sv_stem_filter",
          "icu_folding"
        ],
        "tokenizer": "icu_tokenizer",
        "char_filter": [
          "html_strip",
          "quotes"
        ]
      },
      "fr_analyzer": {
        "type": "custom",
        "filter": [
          "icu_normalizer",
          "elision",
          "fr_stem_filter",
          "icu_folding"
        ],
        "tokenizer": "icu_tokenizer",
        "char_filter": [
          "html_strip",
          "quotes"
        ]
      },
      "ja_analyzer": {
        "type": "custom",
        "filter": [
          "kuromoji_baseform",
          "ja_pos_filter",
          "icu_normalizer",
          "icu_folding",
          "cjk_width"
        ],
        "tokenizer": "kuromoji_tokenizer",
        "char_filter": ["html_strip"]
      },
      "ko_analyzer": {
        "type": "cjk",
        "filter": []
      },
      "zh_analyzer": {
        "type": "custom",
        "filter": [
          "smartcn_word",
          "icu_normalizer",
          "icu_folding"
        ],
        "tokenizer": "smartcn_sentence",
        "char_filter": ["html_strip"]
      },
      "default": {
        "type": "custom",
        "filter": [
          "icu_normalizer",
          "icu_folding"
        ],
        "tokenizer": "icu_tokenizer",
        "char_filter": [
          "html_strip",
          "quotes"
        ]
      },
      "tokenizer": {
        "kuromoji": {
          "type": "kuromoji_tokenizer",
          "mode": "search",
          "char_filter": ["html_strip"]
        }
      }
    }
  },
  "mappings": {
    "document": {
      "dynamic_templates": [
        {
          "es": {
            "match": "*_es",
            "match_mapping_type": "string",
            "mapping": {
              "analyzer": "es_analyzer",
              "type": "string"
            }
          },
          "en": {
            "match": "*_en",
            "match_mapping_type": "string",
            "mapping": {
              "analyzer": "en_analyzer",
              "type": "string"
            }
          },
          "de": {
            "match": "*_de",
            "match_mapping_type": "string",
            "mapping": {
              "analyzer": "de_analyzer",
              "type": "string"
            }
          },
          "fi": {
            "match": "*_fi",
            "match_mapping_type": "string",
            "mapping": {
              "analyzer": "fi_analyzer",
              "type": "string"
            }
          },
          "fr": {
            "match": "*_fr",
            "match_mapping_type": "string",
            "mapping": {
              "analyzer": "fr_analyzer",
              "type": "string"
            }
          },
          "hu": {
            "match": "*_hu",
            "match_mapping_type": "string",
            "mapping": {
              "analyzer": "hu_analyzer",
              "type": "string"
            }
          },
          "it": {
            "match": "*_it",
            "match_mapping_type": "string",
            "mapping": {
              "analyzer": "it_analyzer",
              "type": "string"
            }
          },
          "ja": {
            "match": "*_ja",
            "match_mapping_type": "string",
            "mapping": {
              "analyzer": "ja_analyzer",
              "type": "string"
            }
          },
          "ko": {
            "match": "*_ko",
            "match_mapping_type": "string",
            "mapping": {
              "analyzer": "ko_analyzer",
              "type": "string"
            }
          },
          "pt": {
            "match": "*_pt",
            "match_mapping_type": "string",
            "mapping": {
              "analyzer": "pt_analyzer",
              "type": "string"
            }
          },
          "ru": {
            "match": "*_ru",
            "match_mapping_type": "string",
            "mapping": {
              "analyzer": "ru_analyzer",
              "type": "string"
            }
          },
          "sv": {
            "match": "*_sv",
            "match_mapping_type": "string",
            "mapping": {
              "analyzer": "sv_analyzer",
              "type": "string"
            }
          },
          "zh": {
            "match": "*_zh",
            "match_mapping_type": "string",
            "mapping": {
              "analyzer": "zh_analyzer",
              "type": "string"
            }
          },
          "string_fields": {
            "mapping": {
              "analyzer": "default",
              "type": "string"
            },
            "match_mapping_type": "string",
            "match": "*"
          }
        }
      ],
      "properties": {
        "created": {"type": "date"},
        "updated": {"type": "date"},
        "document_id": {
          "type": "string",
          "index": "not_analyzed"
        },
        "language": {
          "type": "string",
          "index": "not_analyzed"
        },
        "path": {
          "type": "string",
          "index": "not_analyzed"
        },
        "promote": {"type": "boolean"}
      },
      "_all": {"enabled": false}
    }
  }
}